// Тест рекурсивной функции Фибоначчи
fn fibonacci(n) {
    if n <= 1 {
        return n
    }
    return fibonacci(n - 1) + fibonacci(n - 2)
}

println("fibonacci(5) = " + fibonacci(5))
println("fibonacci(10) = " + fibonacci(10))
println("fibonacci(15) = " + fibonacci(15))

// Этот вызов может быть медленным но не должен переполнить стек
println("fibonacci(20) = " + fibonacci(20))

println("Тест Фибоначчи завершен успешно!")