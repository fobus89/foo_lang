// Простой тест системы интерфейсов

println("=== Simple Interface Demo ===")

// Определяем интерфейс
interface Drawable {
    fn draw()
    fn getArea() -> float
}

println("Interface Drawable defined!")

// Определяем структуру
struct Circle {
    radius: float
}

println("Struct Circle defined!")

// Реализуем интерфейс
impl Drawable for Circle {
    fn draw() {
        println("Drawing a circle with radius " + this.radius.toString())
    }
    
    fn getArea() -> float {
        return 3.14159 * this.radius * this.radius
    }
}

println("Interface implemented!")

// Создаем объект и тестируем
let circle = Circle{radius: 5.0}
println("\n=== Testing Circle ===")
circle.draw()
println("Area: " + circle.getArea().toString())

println("\n=== Interface Demo Complete ===")