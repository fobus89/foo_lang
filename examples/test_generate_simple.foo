// Простой тест генерации без сложных шаблонов

struct User {
    name: string,
    age: int
}

macro generatePrint(structParam: StructType) {
    println("=== Macro-time: Processing " + structParam.Name + " ===")
    
    generate {
        fn printUser(user) {
            println("User name: " + user.name)
            println("User age: " + user.age)
        }
    }
}

println("=== Testing simple generate block ===")

// Вызываем макрос
@generatePrint(User)

// Используем сгенерированную функцию
let john = User{name: "John", age: 30}
printUser(john)