println("=== Пошаговая диагностика ===")

let x = 10
const PI = 3.14159
x = x + 5
println("x = " + x)
println("PI = " + PI)

println("Создание squares:")
const squares = for let a = 1; a <= 5; a++ {
    yield a * a
}
println("squares готов")
println(squares)

println("Создание evens:")
const evens = for let b = 0; b < 10; b++ {
    if b % 2 == 0 {
        yield b
    }
}
println("evens готов")
println(evens)