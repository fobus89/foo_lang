// Отладка макросов

struct User {
    name: string,
    age: int
}

// Макрос только с macro-time кодом
macro debugAnalyze(typeInfo) {
    println("Debug: Type = " + typeInfo.Name)
    println("Debug: Kind = " + typeInfo.Kind)
}

let userType = type(User)
@debugAnalyze(userType)

println("Debug completed!")