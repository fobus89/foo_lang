println("Тест match:")

let x = 2

match x {
    1 => println("x равен 1")
    2 => println("x равен 2")
    3 => println("x равен 3")
    _ => println("x не равен 1, 2 или 3")
}

let day = "понедельник"

match day {
    "понедельник" => println("Начало недели")
    "пятница" => println("Конец рабочей недели")
    "суббота" => println("Выходной!")
    "воскресенье" => println("Выходной!")
    _ => println("Рабочий день")
}