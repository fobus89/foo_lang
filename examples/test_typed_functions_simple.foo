// Простой тест типизированных функций

fn add(x: int, y: int) {
    return x + y
}

fn greet(name: string, age: int) {
    return "Hello " + name + ", age " + age
}

println("=== Simple Typed Functions Test ===")

let result1 = add(5, 3)
println("add(5, 3) = " + result1)

let result2 = greet("Alice", 25)
println(result2)

println("Tests completed!")