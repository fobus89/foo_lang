// Простой тест macro-time выполнения

struct User {
    name: string,
    age: int
}

// Простой макрос с macro-time анализом
macro simpleAnalyze(typeInfo) {
    println("=== MACRO-TIME ANALYSIS ===")
    println("Type: " + typeInfo.Name)
    
    Expr {
        println("=== GENERATED CODE ===")  
        println("Processing type: " + typeInfo.Name)
    }
}

let userType = type(User)
@simpleAnalyze(userType)

println("Test completed!")