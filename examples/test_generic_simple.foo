println("=== SIMPLE TYPED FUNCTIONS TEST ===")
println("")

// Простая типизированная функция
fn add(a: int, b: int) {
    return a + b
}

// Функция со строками
fn concat(first: string, second: string) {
    return first + second
}

// Тестируем
let result1 = add(5, 3)
let result2 = concat("Hello", " World")

println("add(5, 3) = " + result1)
println("concat result = " + result2)

println("")
