println("Отладка const:")

println("1. Простая константа:")
const x = 42
println("x = " + x)

println("2. Константа с массивом:")
const arr = for let i = 0; i < 3; i++ {
    yield i
}
println("arr = " + arr)

println("3. Вторая константа с массивом:")
const arr2 = for let j = 5; j < 8; j++ {
    yield j
}
println("arr2 = " + arr2)